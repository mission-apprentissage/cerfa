{
  "name": "mna-cerfa-server",
  "version": "1.9.1",
  "description": "[MNA] Générateur Cerfa Serveer",
  "repository": "https://github.com/mission-apprentissage/cerfa.git",
  "private": true,
  "author": "MNA",
  "license": "MIT",
  "main": "src/index.js",
  "scripts": {
    "start": "nodemon --ignore tests/ src/index.js",
    "debug": "nodemon --inspect=0.0.0.0 --signal SIGINT --ignore tests/ src/index.js",
    "test": "CERFA_LOG_LEVEL=fatal mocha tests",
    "test:coverage": "nyc --temp-dir .coverage/.nyc_output --report-dir .coverage --reporter=lcov --reporter=html yarn test",
    "lint": "eslint src/ tests/",
    "docker:start": "docker-compose up -d --build --force-recreate",
    "docker:stop": "docker-compose down",
    "docker:clean": "docker-compose kill && docker system prune --force --volumes",
    "seed": "node src/jobs/seed/index.js",
    "playground": "node src/jobs/playground/index.js",
    "jobs": "node src/jobs/index.js",
    "clear": "node src/jobs/clear/index.js",
    "stats": "node src/jobs/stats/index.js"
  },
  "dependencies": {
    "axios": "0.24.0",
    "body-parser": "1.19.0",
    "boom": "7.3.0",
    "bunyan": "1.8.12",
    "bunyan-mongodb-stream": "0.0.3",
    "bunyan-prettystream": "0.1.3",
    "bunyan-slack": "0.0.10",
    "clamscan": "2.0.1",
    "cli-progress": "3.8.2",
    "compose-middleware": "5.0.1",
    "convert-csv-to-json": "0.0.15",
    "cookie-parser": "1.4.6",
    "dotenv": "10.0.0",
    "ejs": "3.1.6",
    "env-var": "6.1.1",
    "express": "4.17.1",
    "fs-extra": "9.0.0",
    "global": "4.4.0",
    "iconv-lite": "0.5.1",
    "joi": "17.2.1",
    "json2csv": "5.0.1",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.21",
    "mjml": "4.10.4",
    "moment": "2.29.1",
    "mongoose": "5.13.13",
    "mongoose-paginate": "5.0.3",
    "multiparty": "4.2.2",
    "multipipe": "4.0.0",
    "nodemailer": "6.7.1",
    "nodemailer-html-to-text": "3.2.0",
    "oleoduc": "0.6.0",
    "openid-client": "5.1.0",
    "passport": "0.4.1",
    "passport-http": "0.3.0",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "pdf-lib": "1.17.1",
    "query-string": "6.13.8",
    "rate-limiter-flexible": "2.2.1",
    "sha512crypt-node": "0.1.0",
    "socket.io": "4.4.0",
    "tcp-port-used": "1.0.2",
    "xlsx": "0.17.0"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "devDependencies": {
    "axiosist": "0.10.0",
    "form-data": "4.0.0",
    "mocha": "9.1.3",
    "mongodb-memory-server": "8.0.2",
    "nock": "13.2.1",
    "nodemon": "2.0.15",
    "nyc": "15.1.0"
  }
}
